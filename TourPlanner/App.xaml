<Application x:Class="TourPlanner.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TourPlanner">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/icons.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Images.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/Colors.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/styles/ComboBoxStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/styles/ButtonStyles.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Resources/styles/FormStyles.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
